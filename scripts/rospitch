#!/bin/bash

PRTI1516E_HOME=/opt/prti1516e
JRE_HOME=$PRTI1516E_HOME/jre

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PRTI1516E_HOME/lib/gcc41_64
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PRTI1516E_HOME/lib
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$JRE_HOME/lib/amd64
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$JRE_HOME/lib/amd64/server
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$JRE_HOME/lib/amd64/native_threads

export CLASSPATH=$PRTI1516E_HOME/lib/prti1516e.jar

rosrun rospitch rospitch_node ${1+"$@"}